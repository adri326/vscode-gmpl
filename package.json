{
	"name": "gmpl-fork",
	"displayName": "GMPL Fork",
	"description": "A fork of steffenk1337's gmpl syntax highlight extension",
	"version": "1.0.1",
	"authors": ["steffenk1337", "Shad Amethyst"],
  "publisher": "gmpl-fork",
  "repository": "https://github.com/adri326/vscode-gmpl",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "gmpl",
				"aliases": [
					"GNU MathProg",
					"gmpl"
				],
				"extensions": [
					".gmpl",
					".mod"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "gmpl",
				"scopeName": "source.gmpl",
				"path": "./syntaxes/gmpl.tmLanguage.json"
			}
		]
	}
}
